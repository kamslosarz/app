{
  "name": "kslosarz/app",
  "repositories": [
    {
      "type": "path",
      "url": "../packages/*"
    },
    {
      "type": "path",
      "url": "../framework"
    }
  ],
  "require": {
    "php": ">7.4",
    "framework/event-dispatcher": "dev-master",
    "framework/service-container": "dev-master",
    "framework/collection": "dev-master",
    "framework/router": "dev-master",
    "framework/request": "dev-master",
    "framework/response": "dev-master",
    "framework/container": "dev-master",
    "framework/view": "dev-master",
    "framework/form": "dev-master",
    "framework/builder": "dev-master",
    "framework/validator": "dev-master",
    "framework/flash-messenger": "dev-master",
    "framework/factory": "dev-master",
    "framework/filesystem": "dev-master",
    "framework/console": "dev-master",
    "framework/orm": "dev-master",
    "framework/logger": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Tests\\": "tests/src/",
      "fixture\\": "tests/fixture"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^8.1"
  }
}
